# String divisible by 2
def q0(s,i):
  if (s%2==0):
    print("Accepted")
    print("your string is accepted)
    return;

  if(s%2==1):
    q1(s,i+1)
  else:
    q0(s,i+1)

def q1(s,i):
  if (s%2==1):
    print("Rejected1")
    return;

  if(s%2==1):
    q1(s,i+1)
  else:
    q0(s,i+1)

if __name__ == '__main__':
     s = 123

     q0(s,0)
